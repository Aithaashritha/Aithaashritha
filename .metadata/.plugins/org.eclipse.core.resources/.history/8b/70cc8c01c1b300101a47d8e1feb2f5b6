package com.animal;

// parent class
class Animal
{
	void sound()
	{
		System.out.println("Meow...meoww");
	}
	
}

//Child class

class Cat extends Animal
{
	void sleep()
	{
		System.out.println("Cat sleeps......!");
	}
}

//main class

public class Inheritance
{
	public static void main(String[]args)
	{
		Cat cat= new Cat();
		cat.sound();
		cat.sleep();
		System.out.println("Cat shouted and slept......!");
				
	}
}